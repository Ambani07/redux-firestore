{
  "name": "redux-firestore",
  "version": "1.0.0-alpha.2",
  "description": "Redux bindings for Firestore.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/redux-firestore.min.js",
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "index.d.ts"
  ],
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "npm run build:lib && npm run build:esm && npm run build:typefile",
    "build:typefile": "tsc --declaration --outFile index.d.ts --emitDeclarationOnly",
    "build:lib": "tsc",
    "build:esm": "tsc && tsc -m es6 --outDir lib-esm",
    "watch": "npm run build:es -- --watch",
    "watch:lib": "npm run build:lib -- --watch",
    "watch:commonjs": "npm run build:commonjs -- --watch",
    "test": "mocha -R spec ./test/unit/**",
    "test:cov": "nyc mocha -R spec ./test/unit/** && nyc report --reporter=lcov",
    "codecov": "cat coverage/lcov.info | codecov",
    "lint": "eslint src/**.ts --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\"",
    "prepare": "npm run clean && npm run build",
    "preversion": "npm run clean && npm run build",
    "pre-push": "npm run lint"
  },
  "dependencies": {
    "immer": "3.2.0",
    "lodash": "^4.17.15",
    "reduce-reducers": "^1.0.4"
  },
  "peerDependencies": {
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@types/expect": "^1.20.4",
    "@types/lodash": "^4.14.138",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.4",
    "@types/reduce-reducers": "^0.3.0",
    "@types/yargs": "^13.0.2",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "firebase": "^6.6.0",
    "husky": "^3.0.4",
    "mocha": "^6.2.0",
    "nyc": "^14.1.0",
    "prettier": "1.18.2",
    "redux": "^4.0.1",
    "rimraf": "^3.0.0",
    "sinon": "^7.4.2",
    "sinon-chai": "^3.3.0",
    "ts-mocha": "^6.0.0",
    "typescript": "^3.6.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  },
  "license": "MIT",
  "homepage": "https://github.com/prescottprue/redux-firestore#readme",
  "keywords": [
    "firestore",
    "redux",
    "react-firestore",
    "react-redux-firebase",
    "redux-firebase",
    "react-firebase",
    "firebase"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prescottprue/redux-firestore.git"
  },
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
